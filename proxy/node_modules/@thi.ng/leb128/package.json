  "devDependencies": {
    "@microsoft/api-extractor": "^7.33.7",
    "@thi.ng/testament": "^0.3.7",
    "rimraf": "^3.0.2",
    "tools": "^0.0.1",
    "typedoc": "^0.23.22",
    "typescript": "^4.9.4"
  },
  "keywords": [
    "64bit",
    "bigint",
    "binary",
    "conversion",
    "integer",
    "typedarray",
    "typescript",
    "varint",
    "wasm",
    "webassembly",
    "zig",
    "ziglang"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=12.7"
  },
